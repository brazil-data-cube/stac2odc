{
  "engine_name": "astraea-stac-09-mapper",
  "collection": {
    "fromSTAC": {
      "name": "id",
      "description": "description",
      "metadata.product.name": "familyTitle",
      "measurements": {
        "from": "assets",
        "customMapFunction": {
          "functionName": "functionToMapL8Assets",
          "functionFile": "examples/astraea/custom_functions.py"
        }
      }
    },
    "fromConstant": {
      "metadata_type": "eo",
      "measurements.units": "meters"
    }
  },
  "dataset": {
    "fromSTAC": {
      "measurements": {
        "from": "assets",
        "customMapping": {
          "measurements.$key.path": "href",
          "exclude": [
            "thumb_large",
            "thumb_small"
          ]
        }
      }
    }
  }
}
